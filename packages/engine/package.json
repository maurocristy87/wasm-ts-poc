{
    "name": "@wasm-poc/engine",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "rollup -c",
        "clean": "tsc --build --clean",
        "asbuild:debug": "asc assembly/index.ts --target debug --exportRuntime --runtime incremental --bindings esm",
        "asbuild:release": "asc assembly/index.ts --target release --exportRuntime --runtime incremental --bindings esm",
        "asbuild": "npm run asbuild:debug && npm run asbuild:release"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@rollup/plugin-wasm": "^6.2.2",
        "assemblyscript": "^0.27.31",
        "rollup": "^4.29.1",
        "rollup-plugin-del": "^1.0.1",
        "rollup-plugin-dts": "^6.1.1",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.8.1"
    }
}
